<Window x:Class="W26_PhotoData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
   
        <StackPanel>
            <Image Name="mImage" Width="200" Margin="20" />
            <Label Name="mCamera" HorizontalAlignment="Center" />
            <Label Name="mData" HorizontalAlignment="Center" />
            <Label Name="mISO" HorizontalAlignment="Center" />
        <Button Content="Button" Height="23" Name="btn" Width="75" Click="btn_Click" />
    </StackPanel>
   
</Window>
